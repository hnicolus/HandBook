            Title="HandBook"
            xmlns:local="clr-namespace:HandBook">

    <TabbedPage.ToolbarItems>
     
        <!--<ToolbarItem x:Name="btnBin" Text="Recycle Bin" Order="Secondary" Clicked="BtnBin_Clicked" />-->
